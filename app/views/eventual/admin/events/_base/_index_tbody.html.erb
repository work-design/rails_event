<td><%= model.event_taxon&.name %></td>
<td><%= link_to model.name, { action: 'show', id: model.id } %></td>
<td><%= model.members_count %></td>
<td><%= model.price %></td>
<td><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></td>
<td>
  <%= link_to t('model.admin.event_participants.index.title'), { controller: 'event_participants', event_id: model.id }, class: 'button is-small is-rounded is-light' %>
  <%= link_to({ controller: 'event_items', event_id: model.id }, aria: { label: '课程内容' }, class: 'button is-small is-rounded is-light') do %>
    <i class="fa-solid fa-list"></i>
  <% end %>
</td>
